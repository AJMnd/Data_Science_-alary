SELECT * FROM salary_2012
UNION
SELECT * FROM salary_2013
UNION
SELECT * FROM salary_2014
UNION
SELECT * FROM salary_2015
UNION
SELECT * FROM salary_2016
UNION
SELECT * FROM salary_2017
UNION
SELECT * FROM salary_2018
UNION
SELECT * FROM salary_2019
UNION
SELECT * FROM salary_2020
UNION
SELECT * FROM salary_2021;

CREATE TABLE salary_2012_2021
AS
	SELECT * FROM salary_2012
	UNION
	SELECT * FROM salary_2013
	UNION
	SELECT * FROM salary_2014
	UNION* FROM salary_2012
	UNION
	SELECT * FROM salary_2013
	UNION
	SELECT * FROM salary_2015
	UNION
	SELECT * FROM salary_2016
	UNION
	SELECT * FROM salary_2017
	UNION
	SELECT * FROM salary_2018
	UNION
	SELECT * FROM salary_2019
	UNION
	SELECT * FROM salary_2020
	UNION
	SELECT * FROM salary_2021;
	
select * from salary_2012_2021






