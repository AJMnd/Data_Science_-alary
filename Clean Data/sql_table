-- Creating tables for 2012 wages
CREATE TABLE salary_2012 (
 	State VARCHAR(50) NOT NULL,
 	Occupation VARCHAR(200) NOT NULL,
 	Total_Employees real NOT NULL,
	Hourly_Mean real NOT NULL,
	Annual_Mean real NOT NULL,
	Hourly_Median real NOT NULL,
	Annual_Median real NOT NULL
);
drop table salary_2012

select * from salary_2012

ALTER TABLE salary_2012
ADD COLUMN Year integer NOT NULL DEFAULT 2012;
SELECT * FROM salary_2012

ALTER TABLE salary_2012
ALTER COLUMN Total_Employees TYPE INT,
ALTER COLUMN Annual_Mean TYPE INT,
ALTER COLUMN Annual_Median TYPE INT

select * from salary_2012

